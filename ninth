#include<stdio.h>
int main()
{
    int year;
    printf("write the year you want to know if it is leap year or not");
    scanf("%d",&year);
    if((year%400==0)||(year%4==0&&year%100!=0))
    {
        printf("The entered year %d is a leap year",year);
    }
    else
    {
        printf("The entered year %d is not a leap year",year);
    }
    return 0;
}
